package com.xoriant.banking.poc.model;

import java.math.BigInteger;
import java.time.LocalDateTime;

public class PersonInfo {

	private int personId;
	
	private String personName;
	
	private int genderId;
	
	private LocalDateTime dob;
	
	private BigInteger mobNumber;
	
	private int addressId;
	
	private int role_id;
	
	private String personType;
	
	private int personTypeId;
	

	public PersonInfo() {
		super();
		// TODO Auto-generated constructor stub
	}


	public PersonInfo(int personId, String personName, int genderId, LocalDateTime dob, BigInteger mobNumber,
			int addressId, int role_id, String personType, int personTypeId) {
		super();
		this.personId = personId;
		this.personName = personName;
		this.genderId = genderId;
		this.dob = dob;
		this.mobNumber = mobNumber;
		this.addressId = addressId;
		this.role_id = role_id;
		this.personType = personType;
		this.personTypeId = personTypeId;
	}


	public int getPersonId() {
		return personId;
	}


	public void setPersonId(int personId) {
		this.personId = personId;
	}


	public String getPersonName() {
		return personName;
	}


	public void setPersonName(String personName) {
		this.personName = personName;
	}


	public int getGenderId() {
		return genderId;
	}


	public void setGenderId(int genderId) {
		this.genderId = genderId;
	}


	public LocalDateTime getDob() {
		return dob;
	}


	public void setDob(LocalDateTime dob) {
		this.dob = dob;
	}


	public BigInteger getMobNumber() {
		return mobNumber;
	}


	public void setMobNumber(BigInteger mobNumber) {
		this.mobNumber = mobNumber;
	}


	public int getAddressId() {
		return addressId;
	}


	public void setAddressId(int addressId) {
		this.addressId = addressId;
	}


	public int getRole_id() {
		return role_id;
	}


	public void setRole_id(int role_id) {
		this.role_id = role_id;
	}


	public String getPersonType() {
		return personType;
	}


	public void setPersonType(String personType) {
		this.personType = personType;
	}


	public int getPersonTypeId() {
		return personTypeId;
	}


	public void setPersonTypeId(int personTypeId) {
		this.personTypeId = personTypeId;
	}


	@Override
	public String toString() {
		return "PersonInfo [personId=" + personId + ", personName=" + personName + ", genderId=" + genderId + ", dob="
				+ dob + ", mobNumber=" + mobNumber + ", addressId=" + addressId + ", role_id=" + role_id
				+ ", personType=" + personType + ", personTypeId=" + personTypeId + "]";
	}


	
}
